<viewservice:WindowView
    x:ClassModifier="internal"
    xmlns:viewservice="clr-namespace:AcademyManager.Presentation.WPF.Common.MVVM.ViewService"
    x:Class="AcademyManager.Presentation.WPF.Views.RegisterWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AcademyManager.Presentation.WPF.Views"
        xmlns:usercontrols="clr-namespace:AcademyManager.Presentation.WPF.Views.UserControls"
        mc:Ignorable="d"
        Background="#FF252526"
        Title="RegisterWindow" Height="200" Width="400" WindowStartupLocation="CenterScreen">
    <Grid>
        <usercontrols:RegisterUserControl Grid.Row="0" DataContext="{Binding RegisterUserConrol}"/>
    </Grid>
</viewservice:WindowView>
